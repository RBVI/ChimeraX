<BundleInfo name="ChimeraX-uniprot" version="1.0"
	    package="chimerax.seqlign.uniprot"
  	    minSessionVersion="1" maxSessionVersion="1">

  <Author>UCSF RBVI</Author>
  <Email>chimerax@cgl.ucsf.edu</Email>
  <URL>https://www.rbvi.ucsf.edu/chimerax/</URL>

  <Synopsis>UniProt database support</Synopsis>
  <Description>Fetch Uniprot sequence info</Description>

  <Categories>
    <Category name="Sequence"/>
  </Categories>

  <Dependencies>
    <Dependency name="ChimeraX-Core" version=">=0.9"/>
    <Dependency name="ChimeraX-Alignments" version=">=1.0"/>
  </Dependencies>

  <Classifiers>
    <PythonClassifier>Development Status :: 2 - Pre-Alpha</PythonClassifier>
    <PythonClassifier>License :: Free for non-commercial use</PythonClassifier>
    <ChimeraXClassifier>DataFormat :: UniProt :: :: Sequence :: :: :: https://www.uniprot.org :: :: :: UniProt sequence data</ChimeraXClassifier>
    <ChimeraXClassifier>Fetch :: uniprot :: UniProt :: uniprot :: NGF_HUMAN :: true</ChimeraXClassifier>
  </Classifiers>

</BundleInfo>
