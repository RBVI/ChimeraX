<BundleInfo name="ChimeraX-Clashes" version="1.0" package="chimerax.atomic.clashes"
  	    minSessionVersion="1" maxSessionVersion="1">

  <!-- Additional information about bundle source -->
  <Author>UCSF RBVI</Author>
  <Email>chimerax@cgl.ucsf.edu</Email>
  <URL>https://www.rbvi.ucsf.edu/chimerax/</URL>

  <!-- Synopsis is a one-line description
       Description is a full multi-line description -->
  <Synopsis>Find clashes/contacts in structures</Synopsis>
  <Description>Find atomic clashes/contacts in structures</Description>

  <DataFiles>
    <DataFile>*.so</DataFile>
  </DataFiles>

  <!-- Categories is a list where this bundle should appear -->
  <Categories>
    <Category name="Structure Analysis"/>
  </Categories>

  <!-- Dependencies on other ChimeraX/Python packages -->
  <Dependencies>
    <Dependency name="ChimeraX-Core" version=">=0.8"/>
    <Dependency name="ChimeraX-Atomic" version=">=1.0"/>
    <Dependency name="ChimeraX-Atom_Search" version=">=1.0"/>
  </Dependencies>

  <Classifiers>
    <!-- Development Status should be compatible with bundle version number -->
    <PythonClassifier>Development Status :: 2 - Pre-Alpha</PythonClassifier>
    <PythonClassifier>License :: Free for non-commercial use</PythonClassifier>
    <ChimeraXClassifier>Command :: clashes :: Structure Analysis :: Find atoms with steric clashes</ChimeraXClassifier>
    <ChimeraXClassifier>Command :: contacts :: Structure Analysis :: Finds atoms in contact with one another</ChimeraXClassifier>
    <ChimeraXClassifier>Command :: ~clashes :: Structure Analysis :: Clear clash pseudobond</ChimeraXClassifier>
    <ChimeraXClassifier>Command :: ~contacts :: Structure Analysis :: Clear contact pseudobond</ChimeraXClassifier>
  </Classifiers>

</BundleInfo>
